{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "date_time_immutable": true,
        "final_class": true,
        "final_internal_class": true,
        "global_namespace_import": {
            "import_classes": true,
            "import_constants": true,
            "import_functions": true
        },
        "simplified_null_return": true,
        "braces": false,
        "new_with_braces": {
            "anonymous_class": false,
            "named_class": false
        }
    }
}
